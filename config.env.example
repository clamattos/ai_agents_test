# Exemplo de variáveis de ambiente para o Streamlit Cloud
# Configure estas variáveis no painel do Streamlit Cloud se necessário

# AWS Credentials (opcional - apenas se conectar com Bedrock real)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# Bedrock Agent (opcional)
BEDROCK_AGENT_ID=your_agent_id_here

# Streamlit Configuration
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0

# Application Settings
APP_TITLE="CET-MG - Assistente CNH"
APP_DESCRIPTION="Assistente virtual para emissão e consulta de segunda via de CNH"
